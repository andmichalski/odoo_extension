{
	"manifest_version": 2,
	"name": "Odoo attendances",
	"version": "0.1",
	"content_scripts": [
	  {
	    "matches": [
				"https://odoo.servocode.com/*"
	    ],
	    "js": ["content.js"]
	  }
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"permissions": [
		"https://odoo.servocode.com/*",
		"activeTab",
		"background",
		"cookies"
	],
	"browser_action": {
		"default_popup": "popup.html"
	}
}
